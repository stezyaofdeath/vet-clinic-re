<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.addicted2u.entitys.ServiceEntity" table="medicalServices" catalog="vet_clinic">
        <id name="ID" type="java.lang.String">
            <column name="ms_id" />
        </id>
        <property name="name" type="string">
            <column name="ms_name" length="30" not-null="true" unique="false" />
        </property>
        <property name="cost" type="float">
            <column name="ms_cost" not-null="true" unique="false" />
        </property>
    </class>
</hibernate-mapping>